<!--/Color mask-->
 <!--<div id="color-mask">        </div>--> 
.public #color-mask
{
    z-index: 1;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    overflow-y: auto;
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(-45deg,  rgba(10,152,191,0.6) 1%, rgba(12,153,192,0.6) 2%, rgba(255,255,255,0.6) 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right bottom, color-stop(1%,rgba(10,152,191,0.6)), color-stop(2%,rgba(12,153,192,0.6)), color-stop(100%,rgba(255,255,255,0.6))); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(-45deg,  rgba(10,152,191,0.6) 1%,rgba(12,153,192,0.6) 2%,rgba(255,255,255,0.6) 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(-45deg,  rgba(10,152,191,0.6) 1%,rgba(12,153,192,0.6) 2%,rgba(255,255,255,0.6) 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(-45deg,  rgba(10,152,191,0.6) 1%,rgba(12,153,192,0.6) 2%,rgba(255,255,255,0.6) 100%); /* IE10+ */
    background: linear-gradient(135deg,  rgba(10,152,191,0.6) 1%,rgba(12,153,192,0.6) 2%,rgba(255,255,255,0.6) 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#990a98bf', endColorstr='#99ffffff',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
}






















